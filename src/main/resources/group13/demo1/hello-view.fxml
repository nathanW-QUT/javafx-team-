<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<VBox fx:id="helloView" alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="group13.demo1.controller.HelloController" >
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="welcomeText"/>
    <Label text="Kiernan"/>
    <Label text="Jacob"/>
    <Label text="Nathan"/>
    <Label text="Lisa"/>
    <Label text="vijay"/>


    <HBox spacing="100.0" alignment="CENTER" styleClass=""  >
        <Button fx:id="nextButton" text="Login" onAction="#onLoginButtonClicked"/>

        <Button text="Register" onAction="#onRegisterButtonClicked"/>
    </HBox>
    <HBox alignment="BOTTOM_RIGHT" >
        <Button text="Delete Account" onAction="#onDeleteAccount" fx:id="DeleteAccount" />
    </HBox>
</VBox>
